<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetAllProfessionalsServlet</servlet-name>
        <servlet-class>servlets.GetAllProfessionalsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUsersServlet</servlet-name>
        <servlet-class>servlets.GetAllUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileuploadsservlet</servlet-name>
        <servlet-class>servlets.fileuploadsservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileuploadservlet</servlet-name>
        <servlet-class>servlets.fileuploadservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Adminloginservlet</servlet-name>
        <servlet-class>servlets.Adminloginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Adminlogoutservlet</servlet-name>
        <servlet-class>servlets.Adminlogoutservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllProfessionalsServlet</servlet-name>
        <url-pattern>/getprofessionals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUsersServlet</servlet-name>
        <url-pattern>/getallusers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileuploadsservlet</servlet-name>
        <url-pattern>/fileuploadsservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileuploadservlet</servlet-name>
        <url-pattern>/fileuploadservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Adminloginservlet</servlet-name>
        <url-pattern>/adminloginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Adminlogoutservlet</servlet-name>
        <url-pattern>/adminlogoutservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
